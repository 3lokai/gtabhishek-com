{
  "name": "nextjs-starter-gt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:content": "velite --watch",
    "build:content": "velite --clean",
    "dev:next": "next dev --turbopack",
    "build:next": "next build --turbopack",
    "dev": "run-p dev:*",
    "build": "run-s build:*",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "check": "ultracite check",
    "format": "ultracite format",
    "fix": "ultracite fix",
    "prepare": "husky",
    "supabase:types": "supabase gen types typescript --project-id $NEXT_PUBLIC_SUPABASE_PROJECT_ID > src/types/supabase.ts",
    "velite": "velite"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.1",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.546.0",
    "motion": "^12.23.24",
    "next": "16.0.0",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.10",
    "prisma": "^6.19.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-use-measure": "^2.1.7",
    "recharts": "^3.5.0",
    "resend": "^6.4.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "npm-run-all": "^4.1.5",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "ultracite": "^5.6.4",
    "velite": "^0.3.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "biome check --write",
      "biome format --write"
    ]
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
